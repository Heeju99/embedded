#include "ap_main.h"

Button_Handler_t hBtnMode;
Button_Handler_t hBtnRun;
Button_Handler_t hBtnClear;


int ap_main()
{


   while(1)
   {

   }
   return 0;
}

void ap_Init()
{

   Button_Init(&hBtnMode, GPIOB,  GPIO_PIN_5);
   Button_Init(&hBtnRun, GPIOB, GPIO_PIN_3);
   Button_Init(&hBtnClear, GPIOA, GPIO_PIN_10);

}
