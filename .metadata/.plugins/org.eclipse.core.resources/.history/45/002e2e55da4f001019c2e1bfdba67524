#include "ap_main.h"

Button_Handler_t hBtnMode;
Button_Handler_t hBtnRun;
Button_Handler_t hBtnClear;

enum {WATCH, ST_STOP, ST_RUN, ST_CLEAR};

int ap_main()
{


   while(1)
   {

   }
   return 0;
}

void ap_Init()
{

   Button_Init(&hBtnMode, GPIOB, 5);
   Button_Init(&hBtnRun, GPIOB, 3);
   Button_Init(&hBtnClear, GPIOA, 10);

}
