#include "Model_Mode.h"

typedef enum {S_TIMEWATCH_MODE, S_STOPWATCH_MODE}eModeState_t;

eModeState_t modeState = S_TIMEWATCH_MODE;


 void Model_Mode_Init(eModeState_t state)
 {

 }

 void Model_Set_ModeState(eModeState_t state)
 {
	 modeState = state;
 }

 estopWatchState_t Model_Get_ModeState() //get 함수를 통해 state
 {
	 return modeState;
 }
