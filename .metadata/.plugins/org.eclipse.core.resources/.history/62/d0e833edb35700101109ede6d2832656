#include "StopWatch.h"


stopWatch_t stopWatchData = {0};


void StopWatch_Init()
{

}


void StopWatch_Excute()
{
	estopWatchState_t state = Model_Get_StopWatchState();

	switch(state)
	{
		case S_STOPWATCH_STOP :
			StopWatch_Stop();
			break;

		case S_STOPWATCH_RUN :
			StopWatch_Run();
			break;

		case S_STOPWATCH_CLEAR :
			StopWatch_Clear();
			break;

		default :
			break;
	}
}

void StopWatch_Stop()
{
	stopWatchData.msec = stopWatchData.msec;

}

void StopWatch_Run()
{
	counter++;
}

void StopWatch_Clear()
{
	stopWatchData = 0;
}
