#include "Controller.h"


void Controller_Init()
{
	TimeWatch_Init();
	StopWatch_Init();
}


void Controller_Excute()
{
	eModeState_t state = Model_Get_ModeState();

	switch(state)
	{
	case S_TIMEWATCH_MODE :
		TimeWatch_Excute();
		break;

	case S_STOPWATCH_MODE :
		StopWatch_Excute();
		break;
	}
}

void Controller_CheckMode()
{

}

