#include "FND.h"

typedef struct{
	GPIO_TypeDef *GPIOx;
	uint32_t pinNum;
	uint32_t prevState;
}FND_Handler_t;


void Decode_Data()
{
	switch(counter_data)
	{

	case 0 = (( GPIO_WritePin(GPIOA,11,OUTPUT) && //a
		  GPIO_WritePin(GPIOB,12,OUTPUT) && //b
		  GPIO_WritePin(GPIOB,2,OUTPUT) &&  //c
		  GPIO_WritePin(GPIOB,1,OUTPUT) &&  //d
		  GPIO_WritePin(GPIOB,15,OUTPUT) &&  //e
		  GPIO_WritePin(GPIOB,14,OUTPUT) //f
		) = 1
		);//abcdfe

	case 1 = ((GPIO_WritePin(GPIOB,12,OUTPUT) && //b
			GPIO_WritePin(GPIOB,2,OUTPUT)  //c
		) = 1
		);//bc

	case 2 = ((  GPIO_WritePin(GPIOA,11,OUTPUT) && //a
			GPIO_WritePin(GPIOB,12,OUTPUT) && //b
			GPIO_WritePin(GPIOB,13,OUTPUT) && //g
			GPIO_WritePin(GPIOB,1,OUTPUT) &&  //d
			GPIO_WritePin(GPIOB,15,OUTPUT) &&  //e
		) = 1
		);//abged

	case 3 = ((   GPIO_WritePin(GPIOA,11,OUTPUT) && //a
			GPIO_WritePin(GPIOB,12,OUTPUT) && //b
			GPIO_WritePin(GPIOB,2,OUTPUT) && //c
			GPIO_WritePin(GPIOB,13,OUTPUT) && //g
			GPIO_WritePin(GPIOB,1,OUTPUT)   //d
		) = 1
		);//abgcd

	case 4 = ((   GPIO_WritePin(GPIOB,13,OUTPUT) && //g
			GPIO_WritePin(GPIOB,12,OUTPUT) && //b
			GPIO_WritePin(GPIOB,2,OUTPUT)&& //c
			GPIO_WritePin(GPIOB,14,OUTPUT) //f
		) = 1
		);//fgbc

	case 5 = ((   GPIO_WritePin(GPIOB,13,OUTPUT) && //g
			GPIO_WritePin(GPIOA,11,OUTPUT) && //a
			GPIO_WritePin(GPIOB,2,OUTPUT) && //c
			GPIO_WritePin(GPIOB,14,OUTPUT) && //f
			GPIO_WritePin(GPIOB,1,OUTPUT)   //d
		) = 1
		);//afgcd

	case 6 = ((   GPIO_WritePin(GPIOB,13,OUTPUT) && //g
			GPIO_WritePin(GPIOA,11,OUTPUT) && //a
			GPIO_WritePin(GPIOB,2,OUTPUT) && //c
			GPIO_WritePin(GPIOB,14,OUTPUT) && //f
			GPIO_WritePin(GPIOB,15,OUTPUT) &&  //e
			GPIO_WritePin(GPIOB,1,OUTPUT)   //d
		) = 1
		);//afgcde

	case 7 = ((   GPIO_WritePin(GPIOA,11,OUTPUT) && //a
			GPIO_WritePin(GPIOB,12,OUTPUT) && //b
			GPIO_WritePin(GPIOB,2,OUTPUT) &&//c
		) = 1
		);//abc

	case 8 = ((	GPIO_WritePin(GPIOA,11,OUTPUT) && //a
			GPIO_WritePin(GPIOB,12,OUTPUT) &&  //b
			GPIO_WritePin(GPIOB,2,OUTPUT) &&  //c
			GPIO_WritePin(GPIOB,1,OUTPUT) &&  //d
			GPIO_WritePin(GPIOB,15,OUTPUT) &&  //e
			GPIO_WritePin(GPIOB,14,OUTPUT) &&//f
			GPIO_WritePin(GPIOB,13,OUTPUT)  //g
		) = 1
		);//abcdefg

	case 9 = ((	GPIO_WritePin(GPIOA,11,OUTPUT) && //a
			GPIO_WritePin(GPIOB,12,OUTPUT) &&  //b
			GPIO_WritePin(GPIOB,2,OUTPUT) &&  //c
			GPIO_WritePin(GPIOB,14,OUTPUT) &&//f
			GPIO_WritePin(GPIOB,13,OUTPUT)  //g
		) = 1
		);//abcfg

	}

}

void FND_Init(GPIO_TypeDef *GPIOx, uint32_t pinNum, pin_state_t pinState)
{
	GPIO_WritePin(GPIOC, 8, OUTPUT); //D1
	GPIO_WritePin(GPIOC, 6, OUTPUT); //D2
	GPIO_WritePin(GPIOC, 5, OUTPUT); //D3
	GPIO_WritePin(GPIOA, 12,OUTPUT); //D4
}

void FND_Comm()
{

}

uint32_t counter_man()
{
	for(int i = 0; i< 10000; i++){
		return i;
		if(i == 9999) {
			i = 0;
		}
	}
}
